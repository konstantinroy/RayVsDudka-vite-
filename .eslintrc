{
    "extends": [
      "react-app"
    ],
    "rules": {
        "no-console": "warn",
        "prefer-const": "error",
        "quotes": ["warn", "single"],
        "jsx-quotes": ["warn", "prefer-double"],
        "indent": ["warn", 2],
        "max-len": ["warn", {"code": 120}],
        "comma-dangle": ["warn", "always-multiline"],
        "semi": ["warn", "always"],
        // "import/order": ["warn", {
        //     "groups": ["builtin", "external", "internal",
        //     "parent", "sibling", "index", "object", "type"],
        //     "newlines-between": "always-and-inside-groups"
        // }]
    }
  }